@model IEnumerable<PaperwoffNet.Infrastructure.Asignaturaxtutor>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AsignaturaIdAsignaturaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TutoresIdTutoresNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AsignaturaIdAsignaturaNavigation.IdAsignatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TutoresIdTutoresNavigation.IdTutores)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAxT">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAxT">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAxT">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
