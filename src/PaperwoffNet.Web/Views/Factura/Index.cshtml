@model IEnumerable<PaperwoffNet.Infrastructure.Factura>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IncioPeriodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FinPeriodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalHoras)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TutoresIdTutoresNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncioPeriodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FinPeriodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalHoras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TutoresIdTutoresNavigation.IdTutores)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFactura">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFactura">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFactura">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
