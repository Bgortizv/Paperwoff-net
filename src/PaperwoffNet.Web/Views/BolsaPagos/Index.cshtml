@model IEnumerable<PaperwoffNet.Infrastructure.BolsaPagos>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TotalAbonos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalDescuentos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstudiantesIdEstudiantesNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalAbonos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalDescuentos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstudiantesIdEstudiantesNavigation.IdEstudiantes)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdBolsaPagos">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdBolsaPagos">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdBolsaPagos">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
